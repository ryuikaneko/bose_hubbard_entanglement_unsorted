set xrange [0:]
set yrange [0:]
set xlabel "1/log2(Nsamples)"
set ylabel "entropy density: s/L"
set key left top
set grid
p \
"< cat dat_renyib_L20_t40.0000000000_e*" u (1/$11):($2/$10):($3/$10) w errorl ti "L20", \
"< cat dat_renyib_L40_t80.0000000000_e*" u (1/$11):($2/$10):($3/$10) w errorl ti "L40", \
"< cat dat_renyib_L60_t120.0000000000_e*" u (1/$11):($2/$10):($3/$10) w errorl ti "L60"
pause -1
